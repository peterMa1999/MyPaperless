<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >
	
    <LinearLayout 
		android:layout_width="fill_parent"
		android:layout_height="@dimen/top_bar_height"
		android:orientation="horizontal"
		android:gravity="center_vertical"
		android:background="#3BB715"
		>
	<ImageButton
            android:id="@+id/top_return"
            android:layout_height="@dimen/top_bar_height_35"
            android:layout_width="@dimen/top_bar_height_35"
            android:background="@null"
            android:onClick="returnClick"
            android:src="@drawable/back_click_seletor" />
		
		<TextView
        android:id="@+id/text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="工單點檢修改-選擇修改項"
        android:textColor="#ffffff"
        android:textSize="@dimen/sp_20" />
		
    </LinearLayout>
    <ListView
        android:id="@+id/nospectinlistview"
        android:background="#F5F3F5"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >
    </ListView>
	
</LinearLayout>
